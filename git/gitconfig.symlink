[alias]
    unstage = reset HEAD --
    discard = checkout --

[color]
    branch = auto
    diff = auto
    status = auto
    ui = true

[core]
    autocrlf = input
    excludesfile = ~/.gitignore
    editor = vim

[credential]
    helper = osxkeychain

[difftool]
    prompt = false

[help]
    autocorrect = 1

[hub]
    protocol = https

[mergetool]
    keepBackup = false

[user]
    email = alfonso@al.fon.so
    name = Alfonso Boza
